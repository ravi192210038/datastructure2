#include<stdio.h>
int main(){
    int n,i,fact=1;
    printf("enter the number");
    scanf("%d",&n);
    if(n<0)
    {
           printf("error! factorial of a number does not exist");
           }
           for(i=1;i<=n;i++)
           {
                            fact*=i;
                            }
                            printf("factorial is %d=%llu",n,fact);
                            return 0;}
